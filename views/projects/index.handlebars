
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="col-md-10">
          <h3>Projects</h3>
        </div>
        
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th class='col-md-2'></th>
              <th ></th>
            </tr>
          </thead>
          <tbody>
          {{#each data}}
             
            <tr>
            
              <td class='col-md-2'> <a href="/notes/view/{{this.id}}"><img  src= "{{this.image_url}}"  height="200" width="200"></a></td>
              <td class='col-md-8'>
                  <div>
                    <h4><a href="/notes/view/{{this.id}}"> Name: {{this.name}} </a> </h4>
                 </div>
                  <div> Details: {{this.tagLine}} </div>
                  <div>Location: {{this.location}} </div>
                  <div>Instructions: {{this.instructions}} </div>
                  <div>Announcements: {{this.current_announcements}} </div>
              </td>

              <td class='col-md-2'>
                <form method="get" action="/project/update/{{this.id}}">
                  <button  class="btn btn-default">Edit Project</button>   
                </form>
                
              </td>
            </tr>

          {{/each}}

          </tbody>
        </table> 

      </div>
    </div>
 
    
  
  