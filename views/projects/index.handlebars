
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3>Projects</h3>
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th class='col-md-2'></th>
              <th ></th>
             
            </tr>
          </thead>
          <tbody>
          {{#each data}}
             
          
    
              <tr>
            
              <td class='col-md-2'> <img  src= "{{this.image_url}}"  height="200" width="200"></td>
              <td class='col-md-8'>
                  <div>Name: {{this.name}} </div>
                  <div> Details: {{this.tagLine}} </div>
                  <div>Location: {{this.location}} </div>
                   <div>Instructions: {{this.instructions}} </div>
                    <div>Announcements: {{this.current_announcements}} </div>
                     <div>Replicated From: Project id: {{this.root_project}}</div>
                     <div>Educator Id: {{this.EducatorId}}</div>
                     <div> <button> Edit Project</button>

              </td>
            </tr>

          {{/each}}


          
          </tbody>
        </table> 

      </div>
    </div>
 
    
  
  