
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3>Projects</h3>
      </div>
      <div class="panel-body">
        <table class="table table-striped table-hover ">
          <thead>
            <tr>
              <th>Name</th>
              <th>Location</th>
              <th>Instructions</th>
              <th>Announcements</th>
              <th>Replicated From</th>
            </tr>
          </thead>
          <tbody>
          {{#each data}}
             
          
    
              <tr>
              <td> {{this.name}}</td>
               <td>{{this.location}}</td>
              <td> {{this.instructions}}</td>
              <td> {{this.current_announcements}}</td>
              <td> {{this.root_project}}</td>
              <td > <img  src= "{{this.image_url}}"  height="42" width="42">
            </tr>
          {{/each}}

          
          </tbody>
        </table> 

      </div>
    </div>
 
    
  
  